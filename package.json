{
    "name": "angular2-cookie-law",
    "version": "0.0.0-development",
    "description": "Angular2+ component that provides a banner to inform users about cookie law",
    "scripts": {
        "build": "webpack --config config/webpack.lib.js",
        "build_prod": "npm run clean && npm run inline && npm run ngc && npm run build",
        "clean": "rimraf bundles && rimraf lib",
        "commit": "git-cz",
        "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
        "demo": "webpack-dev-server --inline --progress --open --port 9007 --config config/webpack.demo.js --content-base demo",
        "inline": "ng2-inline -o lib -f -b src src/**/*.ts",
        "lint": "tslint --type-check --project src/tsconfig.json",
        "ngc": "ngc -p tsconfig.json",
        "precommit": "npm run lint && npm run test:headless",
        "semantic-release": "semantic-release pre && npm publish && semantic-release post",
        "test:headless": "npm run test -- --browsers PhantomJS",
        "test": "karma start config/karma.conf.js",
        "test:watch": "npm run test -- --no-single-run"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/andreasonny83/angular2-cookie-law.git"
    },
    "keywords": [
        "ng2",
        "ng4",
        "angular",
        "angular2",
        "angular4",
        "angular2-component",
        "angular4-component",
        "cookie",
        "cookie-law"
    ],
    "author": "Andrea Sonny <andreasonny83@gmail.com> (https://github.com/andreasonny83)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/andreasonny83/angular2-cookie-law/issues"
    },
    "main": "index.js",
    "module": "index.js",
    "typings": "index.d.ts",
    "homepage": "https://github.com/andreasonny83/angular2-cookie-law#readme",
    "peerDependencies": {
        "@angular/common": "6.0.2",
        "@angular/core": "6.0.2",
        "@angular/platform-browser": "6.0.2"
    },
    "devDependencies": {
        "@angular/animations": "6.0.2",
        "@angular/cli": "^6.0.3",
        "@angular/common": "6.0.2",
        "@angular/compiler": "6.0.2",
        "@angular/compiler-cli": "6.0.2",
        "@angular/core": "6.0.2",
        "@angular/platform-browser": "6.0.2",
        "@angular/platform-browser-dynamic": "6.0.2",
        "@types/jasmine": "2.5.46",
        "@types/node": "7.0.5",
        "@types/selenium-webdriver": "2.53.39",
        "@types/source-map": "^0.5.7",
        "@types/tapable": "^1.0.2",
        "@types/webpack": "4.4.0",
        "angular2-inline-template-style": "1.0.2",
        "angular2-template-loader": "0.6.2",
        "awesome-typescript-loader": "3.1.2",
        "codelyzer": "3.0.1",
        "commitizen": "2.9.6",
        "core-js": "2.4.1",
        "coveralls": "2.12.0",
        "cz-conventional-changelog": "2.0.0",
        "husky": "0.14.3",
        "istanbul-instrumenter-loader": "3.0.1",
        "jasmine-core": "2.5.2",
        "karma": "1.5.0",
        "karma-chrome-launcher": "2.2.0",
        "karma-coverage": "1.1.2",
        "karma-jasmine": "1.1.2",
        "karma-mocha-reporter": "2.2.5",
        "karma-phantomjs-launcher": "^1.0.4",
        "karma-remap-coverage": "0.1.5",
        "karma-remap-istanbul": "0.6.0",
        "karma-sourcemap-loader": "0.3.7",
        "karma-webpack": "^3.0.0",
        "phantomjs-prebuilt": "^2.1.16",
        "raw-loader": "0.5.1",
        "rimraf": "2.6.1",
        "rxjs": "^6.1.0",
        "rxjs-compat": "^6.1.0",
        "semantic-release": "6.3.2",
        "source-map": "^0.7.3",
        "source-map-loader": "0.2.0",
        "tslint": "5.0.0",
        "typescript": "^2.7.2",
        "uglify-js": "2.8.16",
        "uglifyjs-webpack-plugin": "0.3.1",
        "webpack": "2.3.2",
        "webpack-dev-server": "2.4.2",
        "zone.js": "^0.8.26"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
